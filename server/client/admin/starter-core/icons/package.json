{
    "name": "@propilot/icons",
    "version": "1.0",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "description": "Custom built bootstrap icons",
    "private": true,
    "scripts": {
        "clean": "rm -rf ./repo && rm -rf ./src",
        "build:icons": "node --loader ts-node/esm ./scripts/build-icons.ts",
        "build:ts": "tsc --project tsconfig.json",
        "build": "npm run clean && rm -rf ./dist && npm run build:icons && npm run build:ts && npm run clean"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.5",
        "babel-loader": "^9.1.2",
        "react": "^18.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4",
        "webpack": "^5.73.0"
    },
    "dependencies": {
        "prettier-eslint": "^16.1.1"
    }
}
